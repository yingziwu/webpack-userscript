{
  "$schema": "http://json-schema.org/draft-07/schema",
  "definitions": {
    "Value": {
      "type": "array",
      "items": { "$ref": "#/definitions/Value" }
    },
    "Switch": { "type": "boolean" },
    "NamedValue": {
      "type": "object",
      "patternProperties": {
        ".*": { "$ref": "#/definitions/Value" }
      }
    },
    "GeneralValue": {
      "anyOf": [
        { "$ref": "#/definitions/Value" },
        { "$ref": "#/definitions/Switch" },
        { "$ref": "#/definitions/Multiple" },
        { "$ref": "#/definitions/NamedValue" }
      ]
    },
    "MonkeyApi": {
      "enum": [
        "unsafeWindow",
        "GM_addStyle",
        "GM_deleteValue",
        "GM_listValues",
        "GM_addValueChangeListener",
        "GM_removeValueChangeListener",
        "GM_setValue",
        "GM_getValue",
        "GM_log",
        "GM_getResourceText",
        "GM_getResourceURL",
        "GM_registerMenuCommand",
        "GM_unregisterMenuCommand",
        "GM_openInTab",
        "GM_xmlhttpRequest",
        "GM_download",
        "GM_getTab",
        "GM_saveTab",
        "GM_getTabs",
        "GM_notification",
        "GM_setClipboard",
        "GM_info"
      ]
    }
  }
}
